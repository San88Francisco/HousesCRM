{
  "search.exclude": {
    "client/.next": true,
    "client/.next/**": true,
    "client/node_modules": true,
    "client/node_modules/**": true,
    "client/.turbo": true,
    "client/.turbo/**": true,
    "client/out": true,
    "client/out/**": true,
    "client/.swc": true,
    "client/.swc/**": true,
    "server/node_modules": true,
    "server/node_modules/**": true,
    "server/dist": true,
    "server/dist/**": true,
    "node_modules": true,
    "**/node_modules": true,
    "**/node_modules/**": true,
    ".next": true,
    "**/.next": true,
    "**/.next/**": true,
    "dist": true,
    "**/dist": true,
    "**/dist/**": true,
    "build": true,
    "**/build": true,
    "**/build/**": true,
    ".turbo": true,
    "**/.turbo": true,
    "**/.turbo/**": true,
    "out": true,
    "**/out": true,
    "**/out/**": true,
    "coverage": true,
    "**/coverage": true,
    "**/coverage/**": true,
    ".git": true,
    "**/.git": true,
    "**/.git/**": true,
    ".eslintcache": true,
    "**/.eslintcache": true,
    "package-lock.json": true,
    "**/package-lock.json": true,
    "yarn.lock": true,
    "**/yarn.lock": true,
    "*.log": true,
    "**/*.log": true,
    ".DS_Store": true,
    "**/.DS_Store": true
  },
  "files.exclude": {
    "client/.next": true,
    "client/node_modules": true,
    "client/.turbo": true,
    "client/out": true,
    "server/node_modules": true,
    "server/dist": true,
    "node_modules": true,
    "**/node_modules": true,
    ".git": true,
    "**/.git": true,
    ".DS_Store": true,
    "**/.DS_Store": true,
    ".eslintcache": true,
    "**/.eslintcache": true
  },
  "files.watcherExclude": {
    "client/.next/**": true,
    "client/node_modules/**": true,
    "client/.turbo/**": true,
    "client/out/**": true,
    "server/node_modules/**": true,
    "server/dist/**": true,
    "node_modules/**": true,
    "**/node_modules/**": true,
    ".git/**": true,
    "**/.git/**": true
  },
  "search.useIgnoreFiles": true,
  "search.useGlobalIgnoreFiles": true,
  "search.useParentIgnoreFiles": true,
  "search.followSymlinks": false,
  "search.smartCase": true,
  "search.maxResults": 5000,
  "typescript.preferences.includePackageJsonAutoImports": "off",
  "postman.settings.dotenv-detection-notification-visibility": false
}
